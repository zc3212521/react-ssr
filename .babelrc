{
  "presets": ["env", "react", "stage-1"],
  "plugins": [
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }],
    ["transform-imports", {
      "lodash": {
        "transform": "lodash/${member}",
        "preventFullImport": true
      }
    }],
    "transform-decorators-legacy",
    "react-hot-loader/babel",
    "css-modules-transform"
  ]
}